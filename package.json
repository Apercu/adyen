{
  "name": "adyen-encrypt",
  "version": "0.1.6",
  "description": "An ES2015 Adyen-CSE rewrite",
  "main": "dist",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && babel src/{aes,index}.js -o dist/tmp.js && cat src/rsa.js >> dist/index.js && cat dist/tmp.js >> dist/index.js && uglifyjs dist/tmp.js -o dist/index.js",
    "sjcl": "(cd ./node_modules/sjcl && ./configure --with-codecBytes && make)",
    "postinstall": "npm run sjcl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apercu/adyen-encrypt.git"
  },
  "author": "Balthazar Gronon <bgronon@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Apercu/adyen-encrypt/issues"
  },
  "homepage": "https://github.com/Apercu/adyen-encrypt#readme",
  "dependencies": {
    "jsencrypt": "^2.1.0",
    "sjcl": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "uglify-js": "^2.6.2"
  }
}
